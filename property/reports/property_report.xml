<odoo>

    <record id="property_report_action" model="ir.actions.report">
        <field name="name">Property Report</field>
        <field name="model">property.property</field>
        <field name="report_type">qweb-html</field>
        <field name="binding_model_id" ref="property.model_property_property"/>
        <field name="binding_type">report</field>
        <field name="report_name">property.property_report_template</field>
    </record>
    <template id="property_report_template">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <t t-foreach="docs" t-as="o">
                    <h1 style="font-family:'ThinItalic'">Usama Fathi</h1>
                </t>
            </t>
        </t>
    </template>

</odoo>